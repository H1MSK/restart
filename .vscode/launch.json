    "configurations": [
        {
            "name": "Python: Generator Test",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Train Humanoid (Py)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "args": [
                "-e",
                "Humanoid",
                "--seed",
                "10000"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            // "preLaunchTask": "rm_run"
        },
        {
            "name": "Python: Train Humanoid (C)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "args": [
                "--env", "Humanoid",
                "--seed", "10000",
                "--model", "c",
                "--test_interval", "0"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            // "preLaunchTask": "rm_run"
        },
        {
            "name": "Python: Run Source Generator",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/models/hlsmodel_src/generator",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/models/hlsmodel_src",
            "justMyCode": false,
            // "preLaunchTask": "rm_run"
        },
        {
            "name": "Python: Train Humanoid (HLS)",
            "type": "python",
            "request": "launch",
            "sudo": true,
            "program": "${workspaceFolder}/train.py",
            "args": [
                "--env", "Humanoid",
                "--seed", "10000",
                "--model", "hls",
                "--test_interval", "0"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            // "preLaunchTask": "rm_run"
        }
    ]
