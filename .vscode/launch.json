    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/models/hlsmodel_src/generated.nn.sim.27.8.64.1.so",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Python: Generator Test",
            "type": "python",
            "request": "launch",
            "program": "generator",
            "cwd": "${workspaceFolder}/models/hlsmodel_src",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Dag Test(Structures)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/models/hlsmodel_src/generator/test_dag_build.py",
            "cwd": "${workspaceFolder}/models/hlsmodel_src/generator",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Dag Test(Onnx)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/models/hlsmodel_src/generator/test_dag_build_from_onnx.py",
            "cwd": "${workspaceFolder}/models/hlsmodel_src/generator",
            "args": ["generated.27.8.64.onnx"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Train Humanoid (Py)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "args": [
                "-e",
                "Humanoid",
                "--seed",
                "10000"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            // "preLaunchTask": "rm_run"
        },
        {
            "name": "Python: Train Humanoid (C)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "args": [
                "--env", "Humanoid",
                "--seed", "10000",
                "--model", "c",
                "--test_interval", "0"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            // "preLaunchTask": "rm_run"
        },
        {
            "name": "Python: Run Source Generator",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/models/hlsmodel_src/generator",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/models/hlsmodel_src",
            "justMyCode": false,
            // "preLaunchTask": "rm_run"
        },
        {
            "name": "Python: Train Humanoid (HLS)",
            "type": "python",
            "request": "launch",
            "sudo": true,
            "program": "${workspaceFolder}/train.py",
            "args": [
                "--env", "Humanoid",
                "--seed", "10000",
                "--model", "hls",
                "--test_interval", "0"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            // "preLaunchTask": "rm_run"
        },
        {
            "name": "Python: Train Ant (HLS)",
            "type": "python",
            "request": "launch",
            "sudo": true,
            "program": "${workspaceFolder}/train.py",
            "args": [
                "--env", "Ant",
                "--seed", "10000",
                "--model", "hls",
                "--max_episode_steps", "512",
                "--test_interval", "0"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            // "preLaunchTask": "rm_run"
        },
        {
            "name": "Python: Train Ant (HLS SIM)",
            "type": "python",
            "request": "launch",
            "sudo": true,
            "program": "${workspaceFolder}/train.py",
            "args": [
                "--env", "Ant",
                "--seed", "10000",
                "--model", "hlssim",
                "--max_episode_steps", "512",
                "--test_interval", "0"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            // "preLaunchTask": "rm_run"
        }
    ]
}